package matrices;

public class Matrix {
	private Integer[][] matrix;
	
	public Matrix(int a, int b){
		matrix = new Integer[a][b];
	}

	public void setPoint(int a,int b, int c){
		matrix[a][b] = c;
	}
	
	public int getPoint(int a, int b){
		return matrix[a][b];
	}
	
	public String toString(){
		for(int i=0; i<matrix.length; i++){
			
		}
		return null;
	}
}
